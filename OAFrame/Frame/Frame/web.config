    <httpRuntime maxRequestLength="102400" requestValidationMode="2.0" />
    <customErrors mode="Off" />
    <compilation debug="true" />

    <authentication mode="Forms">
      <forms name="UserLogin" loginUrl="login.aspx" protection="All" timeout="120" />
    </authentication>
    <authorization>
