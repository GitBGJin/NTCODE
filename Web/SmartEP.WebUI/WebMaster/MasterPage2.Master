<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="MasterPage2.master.cs" Inherits="SmartEP.WebUI.WebMaster.MasterPage2" %>

<%@ Register TagPrefix="telerik" Namespace="Telerik.Web.UI" Assembly="Telerik.Web.UI" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head runat="server">
    <title></title>
    <asp:ContentPlaceHolder ID="head" runat="server">
    </asp:ContentPlaceHolder>
    <style type="text/css">
        html, body {
            overflow: auto;
        }
    </style>
</head>
<body>
    <form id="form1" runat="server">
        <script src='<%=ResolveClientUrl("../Resources/JavaScript/jquery/jquery-1.7.1.min.js") %>' type="text/javascript"></script>
        <script type="text/javascript">
            function fullScreen(mthis) {
                $(mthis).text($(mthis).text() == "全屏" ? "缩小" : "全屏");
                if ($(mthis).text() == "全屏") {
                    $("#tdleft", window.top.document).css("display", "");
                } else {
                    $("#tdleft", window.top.document).css("display", "none");
                }
            }
        </script>
        <div style="height:100%; width:100%; overflow:hidden">
            <telerik:RadScriptManager ID="RadScriptManager1" runat="server" />
            <telerik:RadAjaxLoadingPanel ID="RadAjaxLoadingPanel1" runat="server" />
            <telerik:RadSplitter ID="RadSplitter1" runat="server" ResizeWithBrowserWindow="true" LiveResize="true" Width="100%" Height="100%" Orientation="Horizontal">
                <telerik:RadPane Visible="false" ID="RadPane1" runat="server" Height="24px" Scrolling="None">
                    <div class="RightTopInfo">
                        <div class="icon">
                        </div>
                        <div class="title">
                            <asp:ContentPlaceHolder ID="ContentPlaceHolder3" runat="server">
                            </asp:ContentPlaceHolder>
                        </div>
                        <div class="right">
                            <span class="goback" onclick="history.go(-1)">返回</span><span class="fullscreen" onclick="fullScreen(this)">全屏</span>
                        </div>
                    </div>
                </telerik:RadPane>
                <telerik:RadPane ID="RadPane2" runat="server">
                    <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
                    </asp:ContentPlaceHolder>
                    <asp:ContentPlaceHolder ID="ContentPlaceHolder2" runat="server">
                    </asp:ContentPlaceHolder>
                </telerik:RadPane>
            </telerik:RadSplitter>
        </div>
    </form>
</body>
</html>
