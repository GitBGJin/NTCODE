    <telerik:RadToolBarButton Visible="true" Enabled="true" Text="" Tooltip="增加" Group="Update" CommandName="InitInsert" checkimageurl="images/ImageButton/add.png" ImageUrl="images/ImageButton/add.png"/>
    <telerik:RadToolBarButton Visible="false" Enabled="true" Text="" Tooltip="编辑" Group="Update" CommandName="EditSelected"  checkimageurl="images/ImageButton/edit.png" ImageUrl="images/ImageButton/edit.png"/>
    <telerik:RadToolBarButton Visible="true" Enabled="true" Text="" Tooltip="删除" Group="Update" CommandName="DeleteSelected"  checkimageurl="images/ImageButton/delete.png" ImageUrl="images/ImageButton/delete.png"/>
    <telerik:RadToolBarButton Visible="false" Enabled="false" Text="" Tooltip="保存" Group="Update" CommandName="UpdateEdited"  checkimageurl="images/ImageButton/save.png" ImageUrl="images/ImageButton/save.png"/>
    <telerik:RadToolBarButton Visible="false" Enabled="false" Text="" Tooltip="取消" Group="Update" CommandName="CancelAll"  checkimageurl="images/ImageButton/cancel.png" ImageUrl="images/ImageButton/cancel.png"/>
    <telerik:RadToolBarButton IsSeparator="true" />
    <telerik:RadToolBarButton Visible="true" Enabled="true" Text="" Tooltip="刷新" CommandName="RebindGrid" checkimageurl="images/ImageButton/refresh.png" ImageUrl="images/ImageButton/refresh.png"/>
    <telerik:RadToolBarButton Visible="false" Enabled="true" Text="" Tooltip="查询" CommandName="Query" checkimageurl="images/ImageButton/search.png" ImageUrl="images/ImageButton/search.png"/>
    <telerik:RadToolBarButton Visible="false" Enabled="true" Text="" Tooltip="关闭" CommandName="Close" checkimageurl="images/ImageButton/Menu_OperateExit.gif" ImageUrl="images/ImageButton/Menu_OperateExit.gif"/>
