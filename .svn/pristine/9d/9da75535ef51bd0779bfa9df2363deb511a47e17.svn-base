namespace SmartEP.Service.DataAnalyze.PollutionIndex
{
    using System;
    using System.Collections.Generic;
    using System.Linq;
    using System.Text;

    /// <summary>
    /// 空气质量指数及相关信息
    /// </summary>
    public class PollutionStandard
    {
        #region Fields

        private string color;
        private string colorCode;
        private string healthEffect;
        private string level;
        private string levelType;
        private int? max;
        private int? min;
        private Enums.PollutionIndexType pollutionIndexType;
        private string range;
        private string roman;
        private string suggestion;

        #endregion Fields

        #region Properties

        /// <summary>
        /// 等级对应的颜色名称，如：红色、黄色
        /// </summary>
        public string Color
        {
            get
            {
                return this.color;
            }
            set
            {
                this.color = value;
            }
        }

        /// <summary>
        /// 等级对的颜色(十六进制)，如：#ff0000
        /// </summary>
        public string ColorCode
        {
            get
            {
                return this.colorCode;
            }
            set
            {
                this.colorCode = value;
            }
        }

        /// <summary>
        /// 对健康的影响
        /// </summary>
        public string HealthEffect
        {
            get
            {
                return this.healthEffect;
            }
            set
            {
                this.healthEffect = value;
            }
        }

        /// <summary>
        /// 等级，如：一级、二级
        /// </summary>
        public string Level
        {
            get
            {
                return this.level;
            }
            set
            {
                this.level = value;
            }
        }

        /// <summary>
        /// 等级分类，如：优、良
        /// </summary>
        public string LevelType
        {
            get
            {
                return this.levelType;
            }
            set
            {
                this.levelType = value;
            }
        }

        /// <summary>
        /// 下限
        /// </summary>
        public int? Max
        {
            get
            {
                return this.max;
            }
            set
            {
                this.max = value;
            }
        }

        /// <summary>
        /// 上限
        /// </summary>
        public int? Min
        {
            get
            {
                return this.min;
            }
            set
            {
                this.min = value;
            }
        }

        /// <summary>
        /// 污染指数的类型，如：AQI、API
        /// </summary>
        public Enums.PollutionIndexType PollutionIndex_Type
        {
            get
            {
                return this.pollutionIndexType;
            }
            set
            {
                this.pollutionIndexType = value;
            }
        }

        /// <summary>
        /// 范围
        /// </summary>
        public string Range
        {
            get
            {
                return this.range;
            }
            set
            {
                this.range = value;
            }
        }

        /// <summary>
        /// 等级的罗马字符，如：I、II
        /// </summary>
        public string Roman
        {
            get
            {
                return this.roman;
            }
            set
            {
                this.roman = value;
            }
        }

        /// <summary>
        /// 对健康的建议
        /// </summary>
        public string Suggestion
        {
            get
            {
                return this.suggestion;
            }
            set
            {
                this.suggestion = value;
            }
        }

        #endregion Properties
    }
}