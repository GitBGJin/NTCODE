.RadColorPicker_Neptune {
  font-family: "Segoe UI", Arial, Helvetica, sans-serif;
  /* Rounded corners and shadows */ }
  .RadColorPicker_Neptune .rcpViews,
  .RadColorPicker_Neptune .rcpHeader,
  .RadColorPicker_Neptune .rcpPageView {
    background-color: #dbe6f4; }
