html, body, form {
    height: 100%;
    margin: 0px;
    padding: 0px;
    background-color: none;
}

body {
    background-color: #F5F4E5;
    overflow: hidden;
    width: 100%;
    height: 100%;
    position: static;
    z-index: -10;
}

.divBgImg {
    width: 100%;
    height: 100%;
    /*position: absolute; 
    z-index: -20;*/
    left: 0px;
    top: 0px;
    overflow: hidden;
    margin: auto;
}

.RadTile_Default {
    background-color: none;
    color: #333;
    font-family: "Segoe UI",Arial,Helvetica,sans-serif;
    font-size: 12px;
}

.divWapper {
    background-image: url(../../Resources/Images/HomePage/bg.jpg);
    width: 100%;
    background-repeat:no-repeat;
    background-position: center 0;
    background-size: cover;
    filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../Resources/Images/HomePage/bg',sizingMethod='scale');
    position: absolute;
    top: 0px;
    left: 0px;
}

div.Logo {
    /*margin: auto;
    margin-top: 50px;*/
    width: 600px;
    height: 75px;
    background-image: url('../Images/login/WhiteTopLogo.png');
    background-repeat: no-repeat;
    position: absolute;
    top:10px; 
    left: 0px;
}
div.TitleBg {
    width: 100%;
    height: 75px;
    background: url('../Images/login/title_point.png')  no-repeat center;
    background-size: 100% 100%; 
    display: table;
    top:0px; 
    right: 0px;
}
div.TitleRight {
    width: 500px;
    height: 75px;
    background-image: url('../Images/login/bg_title_right.png');
    background-repeat: no-repeat;
    position: absolute;
    top:0px; 
    right: 0px;
}
div.TitleInfoRight {
    width:100px;
    float:right;
    background-repeat: no-repeat;
    position: absolute;
    top:10px; 
    /*right: 0px;*/
}
div.LogoLZ {
    margin: auto;
    margin-top: 50px;
    width: 627px;
    height: 75px;
    background-image: url('../Images/login/logoLZ.png');
    background-repeat: no-repeat;
    position: relative;
}
div.bg_AQI {
    margin: auto;
    margin-top: 0px;
    width: 402px;
    height: 529px;
    background-image: url('../Images/HomePage/bg_AQI.png');
    background-repeat: no-repeat;
    position: relative;
}
div.divJiCheng div.RadTile {
    background-color: transparent;
}

div.RadTile div.rtileContent {
    background-color: transparent;
}

div.RadTile div.rtilePeekContent {
    background-color: transparent;
}
.images-content{ 
position: absolute; 
z-index: 1; 
left: 0; 
bottom:0; 
width: 100%; 
color: #fff; 
/*background: rgba(0,0,0,.7);*/ 
height: 100px; 
line-height: 100px; 
overflow: hidden; 
font-size:17px;
font-weight:500;
font-family:微软雅黑;
cursor:pointer;
}