.RadTreeView_Outlook {
  color: black;
  font: normal 12px/16px "Segoe UI", Arial, Helvetica, sans-serif; }
  .RadTreeView_Outlook .rtIn {
    background-image: url('Common/radGradientListSprite.png');
    background-image: linear-gradient(transparent, transparent); }
  .RadTreeView_Outlook a.rtIn {
    *color: black; }
  .RadTreeView_Outlook .rtHover .rtIn {
    border-color: #002d96;
    color: black;
    background-color: #ffeec2;
    background-image: linear-gradient(#fff3ca, #ffd294); }
  .RadTreeView_Outlook .rtSelected .rtIn {
    border-color: #002d96;
    color: black;
    background-color: #306ac5;
    background-image: linear-gradient(#dceafc, #97b8e8); }
  .RadTreeView_Outlook .rtEdit .rtIn input {
    *color: black; }
  .RadTreeView_Outlook .rtChecked,
  .RadTreeView_Outlook .rtUnchecked,
  .RadTreeView_Outlook .rtIndeterminate {
    background-image: url('Common/radFormToggleSprite.png'); }
  .RadTreeView_Outlook .rtLoadingIcon {
    background-image: url('Common/loading_small.gif'); }
  .RadTreeView_Outlook .rtPlus,
  .RadTreeView_Outlook .rtMinus {
    background-image: url('TreeView/PlusMinus.png'); }

.RadTreeView_Outlook_disabled .rtIn,
.RadTreeView_Outlook .rtDisabled .rtIn {
  color: #ccc; }
