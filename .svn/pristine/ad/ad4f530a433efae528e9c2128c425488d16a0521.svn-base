.RadTreeMap_Default {
  border-color: #828282;
  color: #333333;
  background-color: white;
  font-size: 12px;
  font-family: "Segoe UI", Arial, Helvetica, sans-serif; }
  .RadTreeMap_Default .k-treemap-tile {
    border-color: #828282;
    background-color: white; }
  .RadTreeMap_Default .k-treemap-title,
  .RadTreeMap_Default .k-treemap-title-vertical {
    border-color: #828282;
    color: #333333;
    background-color: #eaeaea; }
  .RadTreeMap_Default .k-leaf {
    border-color: #ffffff;
    color: white;
    background-color: #333333; }
  .RadTreeMap_Default .k-leaf.k-inverse {
    color: #333333;
    background-color: white; }
  .RadTreeMap_Default .k-leaf.k-state-hover {
    box-shadow: inset 0 0 0 5px white; }
