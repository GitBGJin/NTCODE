@font-face {
  font-family: 'TelerikWebUI';
  src: url('Common/TelerikWebUI.eot');
  src: url('Common/TelerikWebUI.eot') format("embedded-opentype"), url('Common/TelerikWebUI.woff') format("woff"), url('Common/TelerikWebUI.ttf') format("truetype"); }

.RadDataPager {
  min-height: 1.833em;
  padding: 0.250em 0; }
  .RadDataPager:after {
    content: "";
    display: block;
    clear: both;
    visibility: hidden;
    height: 0; }
  .RadDataPager .RadComboBox,
  .RadDataPager .RadInput,
  .RadDataPager .RadInput .riTextBox {
    font-size: 1em; }
  .RadDataPager .rdpWrap,
  .RadDataPager .rdpWrapRight,
  .RadDataPager .rdpWrapNone {
    float: left;
    padding: 0 0.833em;
    line-height: 1.833em;
    white-space: nowrap; }
  .RadDataPager .rdpWrapRight {
    float: right; }
  .RadDataPager .rdpWrapNone {
    float: none; }
  .RadDataPager .rdpWrap a img {
    border: 0;
    margin: 0 8px; }
  .RadDataPager .RadSlider {
    float: left;
    margin: 0 10px 0 0; }
  .RadDataPager .rdpPagerLabel {
    margin: 0 4px 0;
    vertical-align: middle; }
  .RadDataPager .RadComboBox,
  .RadDataPager .RadInput {
    margin: 0 4px 0 0;
    vertical-align: middle; }
  .RadDataPager .RadInput {
    padding-left: 0.01em; }
  .RadDataPager .rdpPagerTextBox {
    text-align: center; }
  .RadDataPager .rdpActionButton {
    width: 1.833em;
    height: 1.833em;
    font-size: 1em;
    margin: 0 0.083em 0 0;
    padding: 0;
    vertical-align: middle;
    cursor: pointer;
    text-align: center;
    background-repeat: no-repeat; }
  .RadDataPager .rdpNumPart a {
    margin: 0;
    padding: 0 0.5em 0;
    line-height: 1.833em;
    text-align: center;
    vertical-align: middle;
    text-decoration: none;
    display: inline-block;
    border: 1px solid transparent;
    border-radius: 2px;
    background-repeat: no-repeat; }
    .RadDataPager .rdpNumPart a.rdpCurrentPage {
      cursor: default; }
  .RadDataPager .rdpPager div.rdpWrap {
    line-height: 32px; }
  .RadDataPager .rdpButtonText {
    display: none; }
  .RadDataPager .rdpIcon:before {
    font-size: 1.4em;
    line-height: normal;
    font-family: "TelerikWebUI";
    text-align: center;
    display: block; }
  .RadDataPager .rdpPagePrev,
  .RadDataPager .rdpPageNext,
  .RadDataPager .rdpPageFirst,
  .RadDataPager .rdpPageLast {
    height: 1.833em;
    width: 1.833em; }
  .RadDataPager .rdpPageFirst .rdpIcon:before {
    content: "\e00b"; }
  .RadDataPager .rdpPagePrev .rdpIcon:before {
    content: "\e003"; }
  .RadDataPager .rdpPageNext .rdpIcon:before {
    content: "\e000"; }
  .RadDataPager .rdpPageLast .rdpIcon:before {
    content: "\e008"; }
  .RadDataPager .rdpPagerButton {
    font-size: 1em;
    height: 1.833em;
    padding: 0 0.3em 0.2em;
    margin: 0 1.167em 0 0;
    vertical-align: middle;
    cursor: pointer;
    border: 1px solid;
    background-repeat: no-repeat; }
  .RadDataPager .rdpAdvPart {
    float: right; }
