  /* Right to Left mode */ }
  .RadToolTip, .RadToolTip .rtTitleWrapper * {
    box-sizing: content-box;
    -moz-box-sizing: content-box; }
  .RadToolTip.rtLoading {
    width: auto;
    height: 100%;
    background-repeat: no-repeat;
    background-position: center center; }
  .RadToolTip.rtRoundedCorner {
    border-radius: 0.41667em; }
  .RadToolTip.rtShadow {
    box-shadow: 2px 3px 4px #828282; }
  .RadToolTip .rtRelativeWrapper {
    position: relative;
    height: 100%; }
  .RadToolTip .rtTitleBar {
    position: relative; }
  .RadToolTip.rtRoundedCorner .rtTitleBar {
    border-radius: 0.41667em 0.41667em 0em 0em; }
  .RadToolTip .rtTitleBar .rtTitleWrapper {
    padding: 0.33333em 0.83333em; }
  .RadToolTip .rtTitleWrapper .rtTitle {
    margin: 0;
    padding: 0;
    font-size: 1.33333em;
    font-weight: normal;
    display: inline-block;
    width: 90%;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
    overflow: hidden;
    border: 0; }
  .RadToolTip .rtClose {
    position: absolute;
    top: -0.83333em;
    right: -0.83333em;
    border: 0;
    border-radius: 45px;
    display: inline-block;
    width: 18px;
    height: 18px;
    text-indent: -9999px;
    background-position: 0 -249px;
    background-repeat: no-repeat;
    background-color: transparent; }
  .RadToolTip .rtClose:hover {
    cursor: pointer;
    background-position: 0 -269px; }
  .RadToolTip .rtCloseIcon:before {
    display: block;
    text-indent: -0.16667em;
    text-indent: -0.75em \9;
    vertical-align: middle;
    line-height: 1em;
    content: "";
    font-family: "TelerikWebUI";
    font-size: 1.25em; }
  .RadToolTip _:-ms-fullscreen, :root .RadToolTip .rtCloseIcon:before {
    text-indent: -0.75em; }
  @media screen and (-webkit-min-device-pixel-ratio: 0) {
    .RadToolTip .rtCloseIcon:before {
      text-indent: -0.55em; } }
  .RadToolTip .rtContent {
    padding: 0.41667em 0.83333em; }
  .RadToolTip.rtRoundedCorner .rtContent {
    border-radius: 0.41667em; }
  .RadToolTip.rtRoundedCorner .rtTitleBar + .rtContent {
    border-radius: 0em 0em 0.41667em 0.41667em; }
  .RadToolTip .rtCallout {
    position: absolute;
    display: none;
    width: 11px;
    height: 11px;
    line-height: 0;
    font-size: 1px; }
  .RadToolTip.rtVisibleCallout .rtCallout {
