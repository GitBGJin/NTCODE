.RadSearchBox_Silk {
  color: #767676;
  font-size: 13px;
  font-family: Arial, Helvetica, sans-serif;
  line-height: 22px; }
  .RadSearchBox_Silk .rsbInner {
    height: 24px;
    border-color: #d7d7d7;
    color: #555555;
    background-color: #ffffff; }
    .RadSearchBox_Silk .rsbInner.rsbHovered {
      border-color: #36b4cb;
      color: #444444;
      background-color: #ffffff; }
    .RadSearchBox_Silk .rsbInner.rsbFocused {
      border-color: #36b4cb;
      color: #000000;
      background-color: #ffffff; }
    .RadSearchBox_Silk .rsbInner.rsbDisabled {
      border-color: #d7d7d7;
      color: #d7d7d7;
      background-color: #ffffff; }
  .RadSearchBox_Silk .rsbInput {
    height: 22px;
    *line-height: 22px; }
  .RadSearchBox_Silk .rsbEmptyMessage {
    color: #b1b1b1; }
  .RadSearchBox_Silk .rsbButton {
    width: 24px;
    height: 24px;
    border-radius: 2px;
    background-image: url('Common/radPrimaryGradientButtonSprite.png');
    background-image: linear-gradient(transparent, transparent); }
    .RadSearchBox_Silk .rsbButton:hover {
      border-color: #007d93;
      color: #ffffff;
      background-color: #007d93;
      background-image: linear-gradient(#00a9c7 0%, #007d93 100%); }
    .RadSearchBox_Silk .rsbButton:focus {
      border-color: #2292a8;
      color: #ffffff;
      background-color: #2dabc1;
      background-image: linear-gradient(#2dabc1 0%, #2dabc1 100%); }
  .RadSearchBox_Silk .rsbButtonSearch {
    width: 28px;
    height: 28px; }
    .RadSearchBox_Silk .rsbButtonSearch .rsbIcon {
      background-image: url('Common/radActionsSprite.png');
      background-position: -17px -4717px; }
    .RadSearchBox_Silk .rsbButtonSearch:hover .rsbIcon {
      background-position: -117px -4717px; }
    .RadSearchBox_Silk .rsbButtonSearch:focus .rsbIcon {
      background-position: -117px -4717px; }
  .RadSearchBox_Silk .rsbSCInner {
    padding-left: 5px;
    padding-right: 22px;
    background-image: url('Common/radPrimaryGradientButtonSprite.png');
    border-color: #c9c9c9;
    color: #767676;
    background-color: #e3e3e3;
    background-image: linear-gradient(#fefefe 0%, #e3e3e3 100%); }
  .RadSearchBox_Silk .rsbSCIcon {
    background-image: url('Common/radActionsSprite.png');
    background-position: -17px -1067px; }
  .RadSearchBox_Silk .rsbSCHovered {
    border-color: #007d93;
    color: #ffffff;
    background-color: #007d93;
    background-image: linear-gradient(#00a9c7 0%, #007d93 100%); }
    .RadSearchBox_Silk .rsbSCHovered .rsbSCIcon {
      background-position: -117px -1067px; }
  .RadSearchBox_Silk .rsbSCFocused {
    border-color: #2292a8;
    color: #ffffff;
    background-color: #2dabc1;
    background-image: linear-gradient(#2dabc1 0%, #2dabc1 100%); }
    .RadSearchBox_Silk .rsbSCFocused .rsbSCIcon {
      background-position: -117px -1067px; }
  .RadSearchBox_Silk .rsbSCDisabled {
    border-color: #8cbbc3;
    color: #d7d7d7;
    background-color: #8cbbc3;
    background-image: linear-gradient(#c1e6ed 0%, #8cbbc3 100%); }
  .RadSearchBox_Silk .rsbLoadingIcon {
    background-image: url('Common/loading_small.gif'); }

.rsbPopup_Silk {
  border-color: #d7d7d7;
  color: #555555;
  background-color: #ffffff;
  font-size: 13px;
  font-family: Arial, Helvetica, sans-serif;
  line-height: 1.5; }
  .rsbPopup_Silk .rsbHeader,
  .rsbPopup_Silk .rsbFooter {
    border-color: #828282;
    color: #5f5f5f;
    background-color: #eaeaea; }
  .rsbPopup_Silk .rsbShowAll {
    border-color: #828282;
    color: #5f5f5f;
    background-color: #eaeaea; }
  .rsbPopup_Silk .rsbListItemHovered {
    color: #555555;
    background-color: #bbeaf3; }
  .rsbPopup_Silk .rsbListItemSelected {
    color: #000000;
    background-color: #fff1cc; }
  .rsbPopup_Silk .rsbListItemLoading {
    color: #555555;
    background-color: #bbeaf3; }
