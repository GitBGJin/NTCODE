.RadTreeMap_Silk {
  border-color: #828282;
  color: #767676;
  background-color: white;
  font-size: 13px;
  font-family: Arial, Helvetica, sans-serif; }
  .RadTreeMap_Silk .k-treemap-tile {
    border-color: #828282;
    background-color: white; }
  .RadTreeMap_Silk .k-treemap-title,
  .RadTreeMap_Silk .k-treemap-title-vertical {
    border-color: #828282;
    color: #5f5f5f;
    background-color: #eaeaea; }
  .RadTreeMap_Silk .k-leaf {
    border-color: #ffffff;
    color: white;
    background-color: #767676; }
  .RadTreeMap_Silk .k-leaf.k-inverse {
    color: #767676;
    background-color: white; }
  .RadTreeMap_Silk .k-leaf.k-state-hover {
    box-shadow: inset 0 0 0 5px white; }
